<!DOCTYPE html>
<html>
<head>
  <title>DataImporter</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<!-- navigation -->
	<%= render :partial => 'shared/navigation' %>

	<!-- main container -->
	<div class="container">
		<div class="flashes">
			<%= flashes_helper %>		
		</div>
		
		<div class="content">
			<div class="row">
				<div class="col-lg-12">
					<div class="main-image"><%#= image_tag '/assets/background.jpg' %></div>	
					<%= yield %>
				</div>	
			</div>	
		</div>
	</div>

</body>
</html>
