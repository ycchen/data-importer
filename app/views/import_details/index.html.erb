<h1>Import details</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Purchaser name</th>
      <th>Merchant name</th>
      <th>Merchant address</th>
      <th>Item description</th>
      <th>Item price</th>
      <th>Purchase count</th>
      <th>Import id</th>
    </tr>
  </thead>

  <tbody>
    <% @import_details.each do |import_detail| %>
      <tr>
        <td><%= import_detail.purchaser_name %></td>
        <td><%= import_detail.merchant_name %></td>
        <td><%= import_detail.merchant_address %></td>
        <td><%= import_detail.item_description %></td>
        <td><%= import_detail.item_price %></td>
        <td><%= import_detail.purchase_count %></td>
        <td><%= import_detail.import.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
